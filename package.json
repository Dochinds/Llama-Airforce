{
  "name": "llama-airforce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "lint laf": "eslint src/Apps/LlamaAirforce/**/*.{ts,vue}",
    "lint cm": "eslint src/Apps/CurveMonitor/**/*.{ts,vue}",
    "lint pm": "eslint src/Apps/PrismaMonitor/**/*.{ts,vue}",
    "dev laf": "vite src/Apps/LlamaAirforce",
    "dev cm": "vite src/Apps/CurveMonitor",
    "dev pm": "vite src/Apps/PrismaMonitor",
    "build contracts": "typechain --target ethers-v5 --input-dir \"./src/ABI\" --out-dir \"./src/Contracts\" \"./src/ABI/**/*.json\"",
    "build laf": "eslint src/Apps/LlamaAirforce/**/*.{ts,vue} && vue-tsc --noEmit && vite build src/Apps/LlamaAirforce",
    "build cm": "eslint src/Apps/CurveMonitor/**/*.{ts,vue} && vue-tsc --noEmit && vite build src/Apps/CurveMonitor",
    "build pm": "eslint src/Apps/PrismaMonitor/**/*.{ts,vue} && vue-tsc --noEmit && vite build src/Apps/PrismaMonitor",
    "test": "vitest --silent --config src/Apps/LlamaAirforce/vitest.config.ts",
    "union": "node --no-lazy -r ts-node/register -r tsconfig-paths/register \"./src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.ts\"",
    "union migrate": "node --no-lazy -r ts-node/register -r tsconfig-paths/register \"./src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.migrate.ts\""
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@kyvg/vue3-notification": "^3.0.2",
    "@web3-onboard/coinbase": "^2.2.6",
    "@web3-onboard/core": "^2.21.2",
    "@web3-onboard/gnosis": "^2.2.0",
    "@web3-onboard/injected-wallets": "^2.10.7",
    "@web3-onboard/walletconnect": "^2.4.7",
    "apexcharts": "^3.43.0",
    "cross-fetch": "^4.0.0",
    "ethers": "^5.7.2",
    "lightweight-charts": "^4.1.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.6",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.7.2",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5",
    "vue3-apexcharts": "^1.4.4",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@azure/cosmos": "^3.17.3",
    "@gnosis.pm/safe-apps-sdk": "^7.8.0",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@safe-global/safe-core-sdk": "^3.3.5",
    "@safe-global/safe-core-sdk-types": "^1.10.1",
    "@safe-global/safe-core-sdk-utils": "^1.7.4",
    "@safe-global/safe-ethers-lib": "^1.9.4",
    "@safe-global/safe-service-client": "^2.0.3",
    "@typechain/ethers-v5": "^11.1.1",
    "@types/lodash": "^4.14.199",
    "@types/readline-sync": "^1.4.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "barrelsby": "^2.8.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "ethereum-multicall": "^2.19.0",
    "ethereumjs-util": "^7.1.5",
    "process": "^0.11.10",
    "readline-sync": "^1.4.10",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.69.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.3",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.16"
  }
}
